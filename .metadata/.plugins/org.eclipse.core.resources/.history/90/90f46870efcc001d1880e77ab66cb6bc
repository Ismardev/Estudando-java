
public class TestaSomatoria {
	
	
	public static void main (String [] args) {
		
		int contador = 0;
		int total = 0;
		
		while (contador <= 10 ) {
			
			total = total + contador;
			contador ++;
			
			
			System.out.println(contador);
			
		}
		
		System.out.println( "o valor total e de: " + total);
		
	}

}
